{
  "name": "minisuper-customer-webapp-ui",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://minisuper-customer-webapp-ui.herokuapp.com/",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.dev.js --port 8080",
    "test": "echo \"run jest --watch\"",
    "test:ci": "echo \"run jest\"",
    "clean:build": "rimraf ./build && mkdir build",
    "prebuild": "run-p clean:build test:ci",
    "build": "webpack --config webpack.config.prod.js"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "bluebird": "^3.7.2",
    "connected-react-router": "^6.8.0",
    "cross-env": "^5.2.0",
    "eslint-loader": "^4.0.2",
    "history": "^5.0.0",
    "html-webpack-plugin": "^4.3.0",
    "http-server": "^0.12.1",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.9.0",
    "ramda": "^0.26.1",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-redux": "6.0.1",
    "react-router-dom": "5.0.0",
    "redux": "^4.0.5",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-saga": "^1.1.3",
    "socket.io-client": "^2.2.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-preset-react-app": "7.0.2",
    "eslint": "5.15.2",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-react": "7.12.4",
    "style-loader": "0.23.1",
    "css-loader": "2.1.1"
  },
  "engines": {
    "node": ">=8"
  },
  "babel": {
    "presets": [
      "babel-preset-react-app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-debugger": "warn",
      "no-console": "warn",
      "no-unused-vars": "warn",
      "react/prop-types": "off",
      "import/no-unresolved": "off"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "root": true
  }
}
